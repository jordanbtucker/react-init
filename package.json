{
  "name": "react-init",
  "version": "0.1.0",
  "description": "Starting point for React projects",
  "files": [
    "build/"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "coverage": "npm run jest -- --coverage",
    "jest": "react-scripts test",
    "lint": "eslint --fix .",
    "preversion": "npm test",
    "start": "react-scripts start",
    "test": "npm run lint && npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jordanbtucker/react-init.git"
  },
  "author": "Jordan Tucker <jordanbtucker@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jordanbtucker/react-init/issues"
  },
  "homepage": ".",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js",
      "!<rootDir>/src/index.js",
      "!<rootDir>/src/serviceWorker.js"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "^2.1.3",
    "sinon": "^7.2.3",
    "tap": "^12.5.1"
  }
}
